page{
  height: 100%;
}
.container {
  margin: 50rpx 5%;
  width: 90%;
  padding: 40rpx 20rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
  background-color: #e9e8e6;
  border-radius: 20rpx; 
  
}

.view-hearder {
  display: flex;
  flex-direction: row;
  width: 100%;
}

.hearder-name-view {
  display: flex;
  flex-direction: column;
  margin-left: 30rpx;
}

.one-view {
  font-size: 30rpx;
  font-weight: bold;
}

.two-view {
  width: 400rpx;
  font-size: 25rpx;
}

.logo {
  height: 200rpx;
  width: 200rpx;
  border-radius: 20rpx;
}

.title {
  /* background-color: #2c2b27; */
  color: white;
  height: 80rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin: 30rpx 0;
}
.text-gan{
  color: #2c2b27;
}
.action-go{
  width: 100%;
  margin-top: 10rpx;
}
.no-view{
  display: flex;
  flex-direction: column;
  align-content: center;
  align-items: center;
  margin-top: 250rpx;
}
.no-img{
  width: 300rpx;
  height: 300rpx;
}
.content{
  color:#00cc77;
  font-size:30rpx;
  font-weight: bold; 
  margin-top: 30rpx;
}
.btn-view{
  margin-top: 40rpx;
  display: flex;
  flex-direction: row;
}
.btn-commit{
  border-radius: 20rpx;
  background-color:#00cc77;
  color: white; 
  margin:0 10rpx; 
  padding: 20rpx 80rpx;
}